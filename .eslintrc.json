{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "warn",
        "quotes": [
            "error",
            "double"
        ],
        "linebreak-style": "off",
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "no-dupe-class-members": 0
    }
}
